<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="800.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.project.controllers.InventoryController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" />
    </padding>
    <center>
        <SplitPane dividerPositions="0.7353" prefHeight="200.0" prefWidth="1646.0" BorderPane.alignment="CENTER">
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                <TableView fx:id="elementTableView" fixedCellSize="0.0" nodeOrientation="LEFT_TO_RIGHT"
                           prefHeight="458.0" prefWidth="1649.0" styleClass="table-view">
                    <columns>
                        <TableColumn fx:id="elementCode" prefWidth="25.0" styleClass="table-column" text="Code"/>
                        <TableColumn fx:id="elementNom" minWidth="110" prefWidth="25.0" styleClass="table-column"
                                     text="Nom"/>
                        <TableColumn fx:id="elementQuantite" prefWidth="25.0" styleClass="table-column" text="Qte"/>
                        <TableColumn fx:id="elementUnite" prefWidth="25.0" styleClass="table-column" text="Unité"/>
                        <TableColumn fx:id="elementPrixA" prefWidth="25.0" styleClass="table-column"
                                     text="Prix Achat"/>
                        <TableColumn fx:id="elementPrixV" prefWidth="25.0" styleClass="table-column"
                                     text="Prix Vente"/>
                    </columns>
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                    </columnResizePolicy>
                </TableView>
            </HBox>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
                <HBox alignment="CENTER_LEFT" maxWidth="Infinity" prefHeight="29.0">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                    <Label minWidth="75.0" text="Code"/>
                    <TextField fx:id="ajoutcode" prefHeight="29.0" promptText="Code">
                        <HBox.margin>
                            <Insets left="10.0"/>
                        </HBox.margin>
                    </TextField>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="29.0">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0"/>
                    </VBox.margin>
                    <Label minWidth="75.0" text="Nom">
                        <HBox.margin>
                            <Insets/>
                        </HBox.margin>
                    </Label>
                    <TextField fx:id="ajoutnom" prefHeight="29.0" promptText="Nom">
                        <HBox.margin>
                            <Insets left="10.0"/>
                        </HBox.margin>
                    </TextField>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="29.0">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0"/>
                    </VBox.margin>
                    <Label minWidth="85.0" text="Achat">
                        <HBox.margin>
                            <Insets/>
                        </HBox.margin>
                    </Label>
                    <TextField fx:id="ajoutprixa" prefHeight="29.0" promptText="Prix achat">
                        <HBox.margin>
                            <Insets/>
                        </HBox.margin>
                    </TextField>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="29.0">
                    <VBox.margin>
                        <Insets bottom="30.0" left="10.0" right="10.0"/>
                    </VBox.margin>
                    <Label minWidth="85.0" text="Vente">
                        <HBox.margin>
                            <Insets/>
                        </HBox.margin>
                    </Label>
                    <TextField fx:id="ajoutprixv" prefHeight="29.0" promptText="Prix vente"/>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="29.0">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0"/>
                    </VBox.margin>
                    <Label minWidth="60.0" text="Qte">
                        <HBox.margin>
                            <Insets/>
                        </HBox.margin>
                    </Label>
                    <TextField fx:id="ajoutqte" maxWidth="207.0" prefHeight="29.0" promptText="Quantité">
                        <HBox.margin>
                            <Insets/>
                        </HBox.margin>
                    </TextField>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="29.0">
                    <VBox.margin>
                        <Insets bottom="20.0" left="10.0" right="10.0"/>
                    </VBox.margin>
                    <ComboBox fx:id="ajoutunite" prefHeight="29.0" promptText="Unité">
                        <HBox.margin>
                            <Insets left="60.0"/>
                        </HBox.margin>
                    </ComboBox>
                </HBox>
                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                    <VBox.margin>
                        <Insets bottom="20.0" top="20.0"/>
                    </VBox.margin>
                    <VBox prefHeight="200.0" prefWidth="150.0">
                        <HBox.margin>
                            <Insets right="20.0"/>
                        </HBox.margin>
                        <padding>
                            <Insets left="20.0"/>
                        </padding>
                        <Button mnemonicParsing="false" onAction="#clearTextField" prefHeight="29.0"
                                prefWidth="200.0" styleClass="action-button-clear" text="Réinitialiser">
                            <VBox.margin>
                                <Insets bottom="20.0"/>
                            </VBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#modifyElement" prefHeight="29.0"
                                prefWidth="200.0" styleClass="action-button-mod" text="Modifier">
                            <VBox.margin>
                                <Insets/>
                            </VBox.margin>
                        </Button>
                    </VBox>
                    <VBox prefHeight="200.0" prefWidth="150.0">
                        <padding>
                            <Insets right="20.0"/>
                        </padding>
                        <Button mnemonicParsing="false" onAction="#addElement" prefHeight="29.0"
                                prefWidth="200.0" styleClass="action-button-add" text="Ajouter"
                                textAlignment="CENTER">
                            <VBox.margin>
                                <Insets bottom="20.0"/>
                            </VBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#delElement" prefHeight="29.0"
                                prefWidth="200.0" styleClass="action-button-del" text="Supprimer">
                            <VBox.margin>
                                <Insets/>
                            </VBox.margin>
                        </Button>
                    </VBox>
                </HBox>
                <Label fx:id="message" styleClass="message" text="">
                    <VBox.margin>
                        <Insets bottom="30.0" top="10.0"/>
                    </VBox.margin>
                </Label>
            </VBox>
        </SplitPane>
    </center>
    <top>
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="120.0" BorderPane.alignment="CENTER">
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                <Button styleClass="nav-button" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                        minWidth="-Infinity" mnemonicParsing="false" onAction="#goToHome" prefHeight="50.0"
                        prefWidth="105.0" text="Accueil" textAlignment="CENTER" textOverrun="LEADING_ELLIPSIS"
                        wrapText="true" VBox.vgrow="ALWAYS"/>
            </VBox>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                <Button styleClass="nav-button" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
                        minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                        onAction="#goToInventaire" prefHeight="50.0" prefWidth="157.0" text="Inventaire"
                        textAlignment="CENTER" textOverrun="LEADING_ELLIPSIS" wrapText="true" VBox.vgrow="ALWAYS"/>
            </VBox>
            <VBox alignment="CENTER" prefHeight="100.0" prefWidth="241.0">
                <Button styleClass="nav-button" alignment="CENTER" contentDisplay="CENTER" maxHeight="-Infinity"
                        maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false"
                        onAction="#goToChainProduction" prefHeight="50.0" prefWidth="270.0"
                        text="Chaine de production" wrapText="true"/>
            </VBox>
        </HBox>
    </top>
</BorderPane>
